{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "outputDirectory": "dist/client",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/server.ts"
    }
  ],
  "functions": {
    "server.ts": {
      "runtime": "nodejs20.x"
    }
  }
}

